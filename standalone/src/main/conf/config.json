{
  "service.discovery.strategy": {
    "user" : "random",
    "device" : "round_robin"
  },
  "verticles" : {
    "ServiceDiscoveryVerticle" : {
      "class" : "com.edgar.service.discovery.verticle.ServiceDiscoveryVerticle",
      "instances" : 1,
      "worker" : false,
      "config" : {
        "importer": "zookeeper://localhost:2181",
        "zookeeper.path": "/csst-microservice",
        "consul.scan-period": 2000,
        "weight.increase" : 1,
        "weight.decrease" : 10,
        "strategy": {
          "user" : "random",
          "device" : "round_robin"
        }
      }
    },
    "ShellVertice" : {
      "class" : "io.vertx.ext.shell.ShellVerticle",
      "instances" : 1,
      "worker" : false,
      "config" : {
        "httpOptions": {
          "port" : 4000
        }
      }
    },
    "ApiDefinitionVerticle" : {
      "class" : "com.edgar.direwolves.verticle.ApiDefinitionVerticle",
      "instances" : 1,
      "worker" : false,
      "config" : {
        "project.namespace": "example",
        "api.config.dir": "standalone/src/test/resources/router",
        "api.config.auto_refresh": false
      }
    },
    "ApiDispatchVerticle" : {
      "class" : "com.edgar.direwolves.dispatch.verticle.ApiDispatchVerticle",
      "instances" : 1,
      "worker" : false,
      "config" : {
        "http.port": 9000,
        "project.namespace": "example",
        "long_req_time" : 1000,
        "redis.host": "10.11.0.31",
        "redis.port": 6379,
        "redis.password": "",
        "redis.sub.keys": [
          "__keyevent@0__:expired",
          "__keyevent@0__:del"
        ],
        "keystore.path": "keystore.jceks",
        "keystore.type": "jceks",
        "keystore.password": "secret",
        "jwt.algorithm": "HS512",
        "jwt.audience": "",
        "jwt.issuer": "",
        "jwt.subject": "",
        "token.expires": 1800,
        "jwt.userClaimKey": "userId",
        "jwt.permissionsClaimKey": "permissions",
        "jwt.user.unique": true,
        "timeout.expires": 300,
        "timeout.enable" : false,
        "strict_arg" : true,
        "strict_arg.query.excludes" : ["appKey", "sign", "signMethod", "v", "nonce", "timestamp"],
        "strict_arg.body.excludes" : [],
        "app.origin": [
          {
            "appKey": "RmOI7jCvDtfZ1RcAkea1",
            "appSecret": "dbb0f95c8ebf4317942d9f5057d0b38e",
            "appCode": 0,
            "scope": "all"
          }
        ],
        "app.codeKey" : "companyCode",
        "app.permissionKey" : "scope",
        "app.importer" : {
          "scan-period": 500000,
          "url": "/appkey/import"
        },
        "log.ignore": ["appkey_list"],
        "backend.permitted" : ["18627874615"]
        }
      }
  }
}