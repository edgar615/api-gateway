{
  "name": "device.link.list.1.0.0",
  "method": "GET",
  "path": "/devices/link",
  "authentication": true,
  "scope": "device:link",
  "appkey": true,
  "app_code_vertify": true,
  "url.arg": [
    {
      "name": "limit",
      "default_value": 10,
      "rules": {
        "integer": true,
        "max": 100,
        "min": 1
      }
    },
    {
      "name": "start",
      "default_value": 0,
      "rules": {
        "integer": true
      }
    }
  ],
  "endpoints": [
    {
      "service": "device",
      "method": "GET",
      "name": "list_device_link",
      "type": "http",
      "path": "/devices/link"
    }
  ],
  "request.transformer": [
    {
      "name": "list_device_link",
      "query.add": [
        "userId:$user.userId"
      ],
      "header.add": [
        "x-auth-userId:$user.userId",
        "x-auth-companyCode:$user.companyCode",
        "x-policy-owner:individual"
      ]
    }
  ]
}