{
  "name": "user.get.1.0.0",
  "method": "GET",
  "path": "/users/info",
  "authentication": true,
  "appkey": true,
  "app_code_vertify": true,
  "endpoints": [
    {
      "service": "user",
      "method": "GET",
      "name": "user",
      "type": "http",
      "path": "/users/$user.userId"
    }
  ],
  "request.transformer": [
    {
      "name": "user",
      "header.add": [
        "x-auth-userId:$user.userId",
        "x-auth-companyCode:$user.companyCode",
        "x-policy-owner:individual"
      ]
    }
  ]
}