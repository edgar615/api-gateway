{
  "name": "user.logout.1.0.0",
  "method": "POST",
  "path": "/logout",
  "authentication": true,
  "appkey": true,
  "endpoints": [
    {
      "service": "user",
      "method": "GET",
      "name": "logout",
      "type": "http",
      "path": "/logout"
    }
  ],
  "request.transformer": [
    {
      "name": "logout",
      "query.add": [
        "userId:$user.userId"
      ],
      "header.add": [
        "x-auth-userId:$user.userId",
        "x-auth-companyCode:$user.companyCode",
        "x-policy-owner:individual"
      ]
    }
  ],
  "jwt.clean": true
}