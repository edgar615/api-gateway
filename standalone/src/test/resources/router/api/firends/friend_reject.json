{
  "name": "friend.reject.1.0.0",
  "method": "PUT",
  "path": "/friends/(\\d+)/reject",
  "authentication": true,
  "scope": "friend:write",
  "appkey": true,
  "app_code_vertify": true,
  "endpoints": [
    {
      "service": "user",
      "method": "PUT",
      "name": "reject_friends",
      "type": "http",
      "path": "/friends/$var.param0/reject"
    }
  ],
  "request.transformer": [
    {
      "name": "reject_friends",
      "header.add": [
        "x-auth-userId:$user.userId",
        "x-auth-username:$user.username",
        "x-auth-companyCode:$user.companyCode",
        "x-auth-fullname:$user.fullname",
        "x-auth-tel:$user.tel",
        "x-auth-mail:$user.mail",
        "x-auth-type:$user.type",
        "x-policy-owner:individual"
      ]
    }
  ]
}